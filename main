#include <iostream>
using namespace std;


int main() {
    cout << "how many temperatures would you like:" << endl;

    int size;
    cin >> size;
    double temperatures[size];

    cout << "Enter temperatures:" << endl;

    for (int i = 0; i < size; i++) {
        cin >> temperatures[i];
    }

    double average;

    for (int i = 0; i < size; i++) {
        average += temperatures[i];
    }

    average /= size;

    cout << average << endl;

    return 0;
}
